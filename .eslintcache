ar\"><strong>{props.username+\":\"}</strong>{\" \"+props.caption}</h4>\n\n            {/* others' comments */}\n            <div>\n                <h5 className=\"h5__combar\">\n                {\n                    comments.map((comment) => (\n                       <p>\n                           <strong>{comment.username} </strong>{comment.text}\n                       </p>\n                    ))\n                }\n                </h5>\n            </div>\n\n            {/* comments section */}\n            <form className=\"form__commentsection\">\n                <input\n                type=\"text\"\n                placeholder=\"Add Comment...\"\n                value={comment}\n                onChange={event => setComment(event.target.value)}\n                className=\"input__commentsection\"\n                />\n                <button\n                disabled={!comment}\n                type=\"submit\"\n                onClick={postComment}\n                className=\"button__commentsection\"\n                >Post\n                </button>\n            </form>\n        </div>\n    )\n}\n\nexport default Post\n","/home/xpera/Documents/instagram clone folder/instagram-clone-react/src/firebase.js",[],"/home/xpera/Documents/instagram clone folder/instagram-clone-react/src/components/ImageUpload.jsx",[],["36","37"],{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","replacedBy":"41"},{"ruleId":"42","severity":1,"message":"43","line":96,"column":9,"nodeType":"44","endLine":99,"endColumn":11},{"ruleId":"42","severity":1,"message":"43","line":119,"column":13,"nodeType":"44","endLine":123,"endColumn":15},{"ruleId":"42","severity":1,"message":"43","line":155,"column":13,"nodeType":"44","endLine":159,"endColumn":15},{"ruleId":"42","severity":1,"message":"43","line":58,"column":13,"nodeType":"44","endLine":61,"endColumn":15},{"ruleId":"38","replacedBy":"45"},{"ruleId":"40","replacedBy":"46"},"no-native-reassign",["47"],"no-negated-in-lhs",["48"],"jsx-a11y/alt